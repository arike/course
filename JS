// set variable 'Celvin' to 294 as a const
const kelvin = 294
// substracting 273 to the Kelvin variable to obtain Celsius
const Celsius = kelvin - 273;
// convert Celsius to Fahrenheit
const Fahrenheit = Celsius * (9/5) + 32;
//fahrenheit round
const fahrenheit = Math.floor(Fahrenheit);
// printing in the console de value of fahrenheit in a word construction
console.log(`The temperature is ${fahrenheit} degrees fahrenheit.`);
